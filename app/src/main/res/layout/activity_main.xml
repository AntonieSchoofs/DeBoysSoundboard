<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@android:color/background_dark"
    tools:context=".MainActivity">

    <ScrollView
        android:id="@+id/scrollView2"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_weight="1"
        android:scrollbars="vertical"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <TableLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            tools:layout_editor_absoluteX="0dp"
            tools:layout_editor_absoluteY="-30dp">

            <TableRow
                android:id="@+id/buttonRow"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:weightSum="3">

                <LinearLayout
                    android:layout_width="0dp"
                    android:layout_weight="1"
                    android:orientation="vertical"
                    android:weightSum="3">

                    <ImageButton
                        android:id="@+id/adjesButton"
                        android:layout_width="match_parent"
                        android:layout_height="230dp"
                        android:layout_margin="6dp"
                        android:layout_weight="1"
                        android:background="@drawable/chung"
                        android:onClick="playSound" />

                    <ImageButton
                        android:id="@+id/overwatchButton"
                        android:layout_width="match_parent"
                        android:layout_height="230dp"
                        android:layout_margin="6dp"
                        android:layout_weight="1"
                        android:background="@drawable/woutimg"
                        android:contentDescription="TODO"
                        android:onClick="playSound" />

                    <ImageButton
                        android:id="@+id/discotimeButton"
                        android:layout_width="match_parent"
                        android:layout_height="230dp"
                        android:layout_margin="6dp"
                        android:layout_weight="1"
                        android:background="@drawable/discotime"
                        android:onClick="playSound" />

                    <ImageButton
                        android:id="@+id/hannesamaiButton"
                        android:layout_width="match_parent"
                        android:layout_height="230dp"
                        android:layout_margin="6dp"
                        android:layout_weight="1"
                        android:background="@drawable/hannesamai"
                        android:onClick="playSound" />

                    <ImageButton
                        android:id="@+id/sletButton"
                        android:layout_width="match_parent"
                        android:layout_height="230dp"
                        android:layout_margin="6dp"
                        android:layout_weight="1"
                        android:background="@drawable/slet"
                        android:onClick="playSound" />

                </LinearLayout>

                <LinearLayout
                    android:layout_width="0dp"
                    android:layout_weight="1"
                    android:orientation="vertical"
                    android:weightSum="3">

                    <ImageButton
                        android:id="@+id/pukkelpopButton"
                        android:layout_width="match_parent"
                        android:layout_height="230dp"
                        android:layout_margin="6dp"
                        android:layout_weight="1"
                        android:background="@drawable/streets"
                        android:onClick="playSound" />

                    <ImageButton
                        android:id="@+id/geileseksButton"
                        android:layout_width="match_parent"
                        android:layout_height="230dp"
                        android:layout_margin="6dp"
                        android:layout_weight="1"
                        android:background="@drawable/drugshol"
                        android:onClick="playSound" />

                    <ImageButton
                        android:id="@+id/chungHoeButton"
                        android:layout_width="match_parent"
                        android:layout_height="230dp"
                        android:layout_margin="6dp"
                        android:layout_weight="1"
                        android:background="@drawable/chung"
                        android:onClick="playSound" />

                    <ImageButton
                        android:id="@+id/hanneshoekortButton"
                        android:layout_width="match_parent"
                        android:layout_height="230dp"
                        android:layout_margin="6dp"
                        android:layout_weight="1"
                        android:background="@drawable/comingsoon"
                        android:onClick="playSound" />

                    <ImageButton
                        android:id="@+id/quinnpijnButton"
                        android:layout_width="match_parent"
                        android:layout_height="230dp"
                        android:layout_margin="6dp"
                        android:layout_weight="1"
                        android:background="@drawable/quinnpijn"
                        android:onClick="playSound" />

                </LinearLayout>

                <LinearLayout
                    android:layout_width="0dp"
                    android:layout_weight="1"
                    android:orientation="vertical"
                    android:weightSum="3">

                    <ImageButton
                        android:id="@+id/vlaamsbelangButton"
                        android:layout_width="match_parent"
                        android:layout_height="230dp"
                        android:layout_margin="6dp"
                        android:layout_weight="1"
                        android:background="@drawable/vb"
                        android:onClick="playSound" />

                    <ImageButton
                        android:id="@+id/hannesWtfButton"
                        android:layout_width="match_parent"
                        android:layout_height="230dp"
                        android:layout_margin="6dp"
                        android:layout_weight="1"
                        android:background="@drawable/hanneswtf"
                        android:onClick="playSound" />

                    <ImageButton
                        android:id="@+id/hannespizzaButton"
                        android:layout_width="match_parent"
                        android:layout_height="230dp"
                        android:layout_margin="6dp"
                        android:layout_weight="1"
                        android:background="@drawable/hannespizza"
                        android:onClick="playSound" />

                    <ImageButton
                        android:id="@+id/hannesreeButton"
                        android:layout_width="match_parent"
                        android:layout_height="230dp"
                        android:layout_margin="6dp"
                        android:layout_weight="1"
                        android:background="@drawable/hannesree"
                        android:onClick="playSound" />

                    <ImageButton
                        android:id="@+id/sound15Button"
                        android:layout_width="match_parent"
                        android:layout_height="230dp"
                        android:layout_margin="6dp"
                        android:layout_weight="1"
                        android:background="@drawable/comingsoon"
                        android:onClick="playSound" />

                </LinearLayout>


            </TableRow>

        </TableLayout>

    </ScrollView>


</androidx.constraintlayout.widget.ConstraintLayout>